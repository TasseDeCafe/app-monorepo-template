{
  "name": "@template-app/native",
  "main": "expo-router/entry",
  "version": "0.0.1",
  "scripts": {
    "test": "jest",
    "dev:tunnel": "APP_VARIANT=development doppler run -- expo start",
    "start:ngrok": "APP_VARIANT=development doppler run -- expo start --tunnel",
    "start:production-mode": "APP_VARIANT=development doppler run -- expo start --no-dev --minify -c",
    "ios": "./scripts/expo-run-on-device.sh ios",
    "ios:emulator": "APP_VARIANT=development doppler run -- npx expo run:ios",
    "android": "./scripts/expo-run-on-device.sh android",
    "android:emulator": "APP_VARIANT=development doppler run -- npx expo run:android",
    "lint": "eslint . --fix",
    "check:types": "npx expo customize tsconfig.json && tsc --noEmit",
    "sync-env": "tsx scripts/sync-env.ts production",
    "reset": "bash scripts/reset-expo-app.sh",
    "reset:and-run-ios": "bash scripts/reset-expo-app.sh && pnpm ios",
    "reset:and-run-android": "bash scripts/reset-expo-app.sh && cd apps/native && pnpm android",
    "eas:build:dev:ios": "eas build --platform ios --profile development",
    "eas:build:dev:android": "eas build --platform android --profile development",
    "eas:build:dev:all": "eas build --platform all --profile development",
    "eas:build:preview:ios": "pnpm sync-env && eas build --platform ios --profile preview",
    "eas:build:preview:android": "pnpm sync-env && eas build --platform android --profile preview",
    "eas:build:preview:all": "pnpm sync-env && eas build --platform all --profile preview",
    "eas:build:prod:ios": "pnpm sync-env && eas build --platform ios --profile production",
    "eas:build:prod:android": "pnpm sync-env && eas build --platform android --profile production",
    "eas:build:prod:all": "pnpm sync-env && eas build --platform all --profile production --auto-submit",
    "eas:submit:ios": "eas submit --platform ios",
    "eas:submit:android": "eas submit --platform android"
  },
  "jest": {
    "preset": "jest-expo"
  },
