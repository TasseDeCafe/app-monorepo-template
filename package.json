{
  "name": "app-monorepo-template.dev",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "packageManager": "pnpm@10.24.0",
  "scripts": {
    "dev": "turbo run dev",
    "dev:tunnel": "scripts/dev-tunnel.sh",
    "start-tunnels": "scripts/start-tunnels.sh",
    "dev:frontend": "turbo run dev --filter=@template-app/web",
    "dev:backend": "turbo run dev --filter=@template-app/backend",
    "dev:landing": "turbo run dev --filter=@template-app/landing-page",
    "dev:all:native": "concurrently \"pnpm dev:frontend:tunnel\" \"pnpm dev:backend:tunnel\" \"pnpm dev:landing:tunnel\" \"pnpm start\"",
    "dev:all:native:tunnel": "concurrently \"pnpm dev:frontend:tunnel\" \"pnpm dev:backend:tunnel\" \"pnpm dev:landing:tunnel\" \"pnpm start:tunnel\"",
    "dev:frontend:tunnel": "turbo run dev:tunnel --filter=@template-app/web",
    "dev:backend:tunnel": "turbo run dev:tunnel --filter=@template-app/backend",
    "dev:landing:tunnel": "turbo run dev:tunnel --filter=@template-app/landing-page",
    "dev:backend:no-third": "turbo run dev:without-third-parties --filter=@template-app/backend",
    "dev:backend:no-third:tunnel": "turbo run dev:without-third-parties:tunnel --filter=@template-app/backend",
    "prod:backend:install:ffmpeg": "turbo run install:ffmpeg --filter=@template-app/backend",
    "prod:backend:check-changes": "turbo run check:changes --filter=@template-app/backend",
    "prod:backend:sentry": "turbo run sentry:prod --filter=@template-app/backend",
    "prod:backend:run": "turbo run prod --filter=@template-app/backend",
    "prod:backend:build": "turbo run build --filter=@template-app/backend",
    "prod:frontend:build": "turbo run build --filter=@template-app/web",
    "prod:landing:build": "turbo run build --filter=@template-app/landing-page",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "check:types": "turbo run check:types",
    "test": "turbo run test",
    "test:run": "turbo run test:run",
    "clean": "turbo run clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "translate": "pnpm --filter @template-app/i18n translate",
    "sync-env": "./apps/native/scripts/sync-env-if-changed.sh",
    "sync-env:force": "pnpm --filter native sync-env",
    "start": "pnpm --filter native start",
    "start:ngrok": "pnpm --filter native start:ngrok",
    "ios": "pnpm --filter native ios",
    "android": "pnpm --filter native android"
  },
  "devDependencies": {
    "@lingui/cli": "catalog:",
    "@lingui/macro": "catalog:",
    "@react-email/components": "catalog:",
    "@typescript-eslint/eslint-plugin": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "babel-plugin-macros": "catalog:",
    "concurrently": "catalog:",
    "eslint": "catalog:",
    "eslint-config-prettier": "catalog:",
    "eslint-plugin-prettier": "catalog:",
    "prettier": "catalog:",
    "prettier-plugin-tailwindcss": "catalog:",
    "react-email": "catalog:",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:"
  }
}
